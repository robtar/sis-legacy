<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cz.silesnet.model.ServiceBlueprint" table="newservices">
        <id name="id" />
        <property name="customerId" column="customer_id" />
        <property name="name" length="20" />
        <property name="download" />
        <property name="upload" />
        <property name="price" />
        <property name="responsible" column="technik" length="10"/>
        <property name="info" column="poznamka" length="90" />
        <property name="periodFrom" column="period_from" type="timestamp" />
        <property name="billingOn" column="billing_on" type="timestamp" />
    </class>
</hibernate-mapping>
